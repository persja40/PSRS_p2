#!/bin/sh
PPFLAGS="-DUPCXX_ASSERT_ENABLED=0 -DUPCXX_BACKEND=1 -DUPCXX_BACKEND_GASNET_PAR=1 -DUPCXX_LPC_INBOX_lockfree=1 -D_GNU_SOURCE=1 -DGASNET_PAR -D_REENTRANT -I/home/ernest/PSRS_p2/upcxx/gasnet.opt/include -I/home/ernest/PSRS_p2/upcxx/gasnet.opt/include/mpi-conduit -I/home/ernest/PSRS_p2/upcxx/upcxx.O3.gasnet_par.mpi/include"
LDFLAGS="-D_GNU_SOURCE=1 -O3 --param max-inline-insns-single=35000 --param inline-unit-growth=10000 --param large-function-growth=200000 -Wno-unused -Wno-unused-parameter -Wno-address"
LIBFLAGS="-L/home/ernest/PSRS_p2/upcxx/upcxx.O3.gasnet_par.mpi/lib -lupcxx -L/home/ernest/PSRS_p2/upcxx/gasnet.opt/lib -lgasnet-mpi-par -lammpi -lpthread -lrt -L/usr/lib/gcc/x86_64-linux-gnu/5 -lgcc -lm"
CC='gcc'
CXX='g++'
GASNET_CONDUIT='mpi'
GASNET_INSTALL='/home/ernest/PSRS_p2/upcxx/gasnet.opt'
[ "$1" != "" ] && eval echo '$'"$1"
